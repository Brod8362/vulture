{
  "maxThreads": 32,
  "watchers": [
    {
      "name": "notify all posts",
      "subreddit": "all",
      "titleRegex": ".*",
      "contentRegex": ".*",
      "matchEither": false,
      "checkInterval": 30,
      "maxPosts": 20,
      "actions": [
        {
          "type": "notify",
          "arguments": {
            "message": "there's a new post in r/all"
          }
        }
      ]
    }
  ]
}